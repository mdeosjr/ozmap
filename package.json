{
  "name": "oztest",
  "version": "0.0.1",
  "description": "DevOZ - Tech test",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "NODE_ENV=test mocha --require ts-node/register ./src/tests/index.spec.ts --exit",
    "test:coverage": "nyc --reporter=html --reporter=text mocha --require ts-node/register ./src/tests/index.spec.ts --exit",
    "format": "prettier --write \"**/*.{js,ts,json}\"",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "format:check": "prettier --check \"**/*.{js,ts,json}\"",
    "lint:check": "eslint \"**/*.{js,ts,tsx}\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "@typegoose/typegoose": "^11.5.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.3",
    "pino": "^9.6.0",
    "pino-http": "^8.5.1",
    "pino-pretty": "^13.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5",
    "@types/chai": "^5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.18",
    "@types/jsonwebtoken": "^9",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.8.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sinon": "^10.0.17",
    "@types/sinon-chai": "^4.0.0",
    "@types/supertest": "^2.0.13",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "chai": "^5.2.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "prettier": "^3.5.3",
    "sinon": "^19.0.2",
    "sinon-chai": "^4.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@4.7.0"
}
