    "chai": "^4.3.10",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5",
    "@types/chai": "^4.3.6",
    "@types/cors": "^2.8.17",
