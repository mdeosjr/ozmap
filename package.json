    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.3",
    "sinon": "^16.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
